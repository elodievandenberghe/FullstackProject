@model List<BookingSite.ViewModels.Interface.IRootObject<BookingSite.ViewModels.HotelViewModel>>

@foreach (var item in Model)
{
    foreach (var hotel in item.Data)
    {
        <p>@hotel.Name</p>
        <p>@hotel.Address.AddressString</p>
        <img src="@hotel.ImageUrl" alt="Hotel image">
        <a href="@hotel.WebUrl">Book your room!</a>
        <p>--------------------------------------------<br/></p>
    }
}